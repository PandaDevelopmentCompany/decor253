        .review-text { font-size: 14px; }
        .review-text strong { font-weight: 700 !important; }
        .google-logo { position: absolute; left: 0; top: 50%; transform: translateY(-50%); }

        @media ( max-width: 767px ) {            
            .google-review-badge { display: flex; gap: 5px; padding-left: 0; flex-flow: row; align-items: center; }
            .google-logo { position: relative; top: 0; transform: none; }
            .google-logo img { width: 37px; }
            .star { font-size: 17px; width: 17px; height: 17px; }
            .review-text { font-size: 12.5px; }
        }        
    </style>
    
</div></div>
</div></div>




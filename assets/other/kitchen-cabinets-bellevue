<script id="sticky_buttons" data-debloat-delay="1" type="text/debloat-script">

    var btn = jQuery('#sticky_btn');

    jQuery(window).scroll(function() {
		if (jQuery(window).scrollTop() > 400) {
			btn.show();
		} else {
			btn.hide();
		}
    });


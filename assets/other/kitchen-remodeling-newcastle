<script data-minify="1" id="jquery-images-compare-js" type="text/javascript" src="https://kitchenmore.com/wp-content/cache/min/1/ajax/libs/mhayes-twentytwenty/1.0.0/js/jquery.twentytwenty.min.js?ver=1768307750" defer></script>
<script id="jquery-images-compare-script" type="text/javascript">
(function($) {
  $(function () {
    $('.image-compare').removeClass().addClass('image-compare');
    $('.image-compare figcaption').remove();
    $('.image-compare .wp-block-image').children().unwrap();
    var $set = $('.image-compare').children();    
    for(var i=0, len = $set.length; i < len; i+=2){
      $set.slice(i, i+2).wrapAll('<div class="twentytwenty-container"/>');
    }
    $(".twentytwenty-container").twentytwenty();    	


$('.image-compare-slider .wp-block-group__inner-container').addClass('swiper').append('<div class="swiper-button-next"></div><div  class="swiper-button-prev"></div><div  class="swiper-pagination"></div>');
            $('.image-compare-slider .image-compare').removeClass().addClass('swiper-wrapper');
            $('.image-compare-slider .twentytwenty-wrapper').wrap('<div class="swiper-slide" />');
            $('.image-compare-slider').removeClass().addClass('image-compare-slider');
            var swiper = new Swiper(".image-compare-slider .swiper", {
                lazy: false,
                allowTouchMove: false,
                autoHeight: false,
                spaceBetween: 40,
                slidesPerView: 3,
                slidesPerGroup: 3,
                breakpoints: {
                    0: {
                        slidesPerView: 1,
                        slidesPerGroup: 1,
                    },
                    600: {
                        slidesPerView: 2,
                        slidesPerGroup: 2,
                    },
                    992: {
                        slidesPerView: 3,
                        slidesPerGroup: 3,
                    },
                },
                navigation: {
                    nextEl: ".image-compare-slider .swiper-button-next",
                    prevEl: ".image-compare-slider .swiper-button-prev",
                },
                pagination: {
                    el: ".image-compare-slider .swiper-pagination",
                    clickable: true,
                },
                observer: true,
                observeParents: true,
                watchSlidesVisibility: true,
                watchSlidesProgress: true,
            });	

  });  
})(jQuery);

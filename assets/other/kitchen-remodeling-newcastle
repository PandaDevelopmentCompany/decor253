        jQuery(document).ready(function ($) {


            $(document).on("mouseleave", function(event) {

                if (document.cookie.includes("popUpExitIntentTriggered=true")) {
                    return; // Do nothing if the popup was already triggered
                }
                
                if (typeof PUM !== 'undefined' && PUM.open) { // Check if PUM.open exists
                    if (!$('html').hasClass('pum-open')) {
                        PUM.open(3449); // Replace 3449 with your popup ID
                        // Set a cookie to prevent reopening
                        document.cookie = "popUpExitIntentTriggered=true; path=/; max-age=" + (60 * 60 * 24); // Expires in 1 day
                    }
                }

            });

        }); // jQuery
